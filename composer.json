{
    "name": "chrismou/phpdocs-to-db",
    "description": "App for creating an sqlite PHP function database using the PHP documentation SVN repository",
    "keywords": [ "php", "cilex", "docs", "phpdocs", "sqlite" ],
    "license": "Apache-2.0",
    "support": {
        "email": "hello@mou.me",
        "issues": "https://github.com/chrismou/phpdocs-to-db/issues",
        "source": "https://github.com/chrismou/phpdocs-to-db"
    },
    "require": {
        "cilex/cilex": "dev-master",
        "doctrine/dbal": ">=2.2.0,<2.4.0-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "mockery/mockery": "~0.9",
        "danrspencer/phpunit-expect-syntax": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Chrismou\\PhpdocsToDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrismou\\PhpdocsToDb\\Tests\\": "tests"
        }
    }
}
